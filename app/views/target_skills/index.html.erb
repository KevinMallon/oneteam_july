<h1>Listing target_skills</h1>

<table>
  <tr>
    <th>Employee</th>
    <th>Skill</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @target_skills.each do |target_skill| %>
  <tr>
    <td><%= target_skill.employee_id %></td>
    <td><%= target_skill.skill_id %></td>
    <td><%= link_to 'Show', target_skill %></td>
    <td><%= link_to 'Edit', edit_target_skill_path(target_skill) %></td>
    <td><%= link_to 'Destroy', target_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

